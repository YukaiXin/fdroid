<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/color929292" android:state_enabled="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/color979797" />
            <corners android:radius="100dp" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
    <item android:color="@color/color929292" android:state_enabled="false">
        <shape>
            <stroke android:width="1dp" android:color="@color/desc" />
            <corners android:radius="100dp" />
            <solid android:color="@color/transparent" />
        </shape>
    </item>
</selector>